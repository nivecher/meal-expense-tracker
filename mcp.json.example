{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "env": {
        "CONTEXT7_API_KEY": ""
      },
      "enabled": true
    },
    "playwright": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-playwright"],
      "env": {
        "NODE_ENV": "development"
      },
      "enabled": true
    },
    "docker": {
      "command": "docker",
      "args": [
        "mcp",
        "gateway",
        "run",
        "--transport",
        "stdio"
      ],
      "env": {
        "DOCKER_HOST": "unix:///var/run/docker.sock"
      },
      "enabled": false
    }
  }
}
