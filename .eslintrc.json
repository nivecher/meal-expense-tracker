{
  "env": {
    "browser": true,
    "es2021": true,
    "jquery": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "no-param-reassign": ["error", { "props": false }],
    "max-len": ["error", { "code": 100, "ignoreComments": true }]
  },
  "globals": {
    "google": "readonly"
  }
}
